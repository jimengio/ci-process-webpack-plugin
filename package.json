{
  "name": "@jimengio/ci-progress-webpack-plugin",
  "version": "0.1.1",
  "description": "",
  "main": "./lib/progress.js",
  "scripts": {
    "dev": "webpack-dev-server --hot --config webpack/dev.js",
    "watch": "webpack-dev-server --hot --config webpack/dev.js",
    "dll": "rm -rfv webpack/dll/* && time webpack --config webpack/dll-dev.js",
    "dll-release": "time webpack --config webpack/dll-release.js",
    "release": "rm -rfv dist/* && yarn dll-release && time webpack --config webpack/release.js",
    "build": "yarn release",
    "compile": "tsc -d --project tsconfig-compile.json --outDir lib/",
    "prepare": "yarn compile",
    "postinstall": "rm -rfv node_modules/@types/*/node_modules/@types/react"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@jimengio/ruled-router": "^0.2.25",
    "@types/lodash": "^4.14.149",
    "@types/react": "^16.9.31",
    "@types/react-dom": "^16.9.6",
    "@types/webpack": "^4.41.10",
    "cache-loader": "^4.1.0",
    "css-loader": "^3.4.2",
    "dayjs": "^1.8.23",
    "duplicate-package-checker-webpack-plugin": "^3.0.0",
    "emotion": "^10.0.27",
    "file-loader": "^6.0.0",
    "font-awesome": "^4.7.0",
    "fork-ts-checker-webpack-plugin": "^4.1.2",
    "html-webpack-plugin": "^4.0.4",
    "html-webpack-tags-plugin": "^2.0.17",
    "immer": "^9.0.6",
    "lodash": "^4.17.15",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "style-loader": "^1.1.3",
    "thread-loader": "^2.1.3",
    "ts-loader": "^6.2.2",
    "typescript": "^3.8.3",
    "typescript-styled-plugin": "^0.15.0",
    "url-loader": "^4.0.0",
    "webpack": "^4.42.1",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-hud": "^0.1.2"
  },
  "dependencies": {}
}
